msgid "**\"Contrato\"** = Contract"
msgstr "**\"Contrato\"** = Contrato"

#: ../../user-guide/faq.rst:266
msgid "**\"Estimación\"** = Estimate"
msgstr "**\"Estimación\"** = Estimación"

#: ../../user-guide/faq.rst:267
msgid "**\"Concepto\"** = Concept/Line Item"
