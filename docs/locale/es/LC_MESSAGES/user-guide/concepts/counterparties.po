msgstr "Contratos de obras electricas"

#: ../../user-guide/concepts/counterparties.rst:65
msgid "Plumbing subcontracts"
msgstr "Subcontratos de fontanería"

#: ../../user-guide/concepts/counterparties.rst:66
msgid "HVAC installations"
msgstr "Instalaciones de climatización"

